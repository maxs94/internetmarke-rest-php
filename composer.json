{
  "name": "maxs94/internetmarke-rest-php",
  "description": "Deutsche Post INTERNETMARKE REST client for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Steindl",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "Maxs94\\Internetmarke\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
    "phpstan-baseline": "vendor/bin/phpstan analyse -c phpstan.neon --generate-baseline"
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
}
